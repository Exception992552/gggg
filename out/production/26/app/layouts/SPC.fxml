<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.classes.SPCController">
   <children>
      <Button fx:id="getOSButton" mnemonicParsing="false" onAction="#loadgetOS" text="getOS" />
      <Button fx:id="DevConButton" mnemonicParsing="false" onAction="#loadDevCon" text="DevCon" />
      <Button fx:id="tasklistbutton" mnemonicParsing="false" onAction="#loadtasklist" text="tasklist" />
      <Button fx:id="be" mnemonicParsing="false" onAction="#exit" text="Exit" />
   </children>
</VBox>
